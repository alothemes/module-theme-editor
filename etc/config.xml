<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>

        <!-- Absolute Theme Default Values START -->
        <swissup_absolute>
            <background>
                <body_background>linear-gradient(to bottom, #004b6e 0%, #00628e 100%)</body_background>
                <body_backgroundColor>#00628e</body_backgroundColor>
                <body_backgroundRepeat>repeat-x</body_backgroundRepeat>
                <footer_background>linear-gradient(to bottom, #0f769d 0%, #00628e 23%, #00628e 100%)</footer_background>
                <footer_backgroundColor>transparent</footer_backgroundColor>
                <footer_backgroundRepeat>repeat</footer_backgroundRepeat>
                <navigation_background>#ffffff none scroll 0 0</navigation_background>
                <navigation_backgroundColor>#ffffff</navigation_backgroundColor>
                <navigation_backgroundRepeat>repeat</navigation_backgroundRepeat>
                <navigationUl_background>linear-gradient(to bottom, #05739a 0%, #005d8d 16%, #004e81 49%, #004274 86%, #003d6c 100%)</navigationUl_background>
                <navigationUl_backgroundColor>transparent</navigationUl_backgroundColor>
                <navigationUl_backgroundRepeat>repeat</navigationUl_backgroundRepeat>
                <navigationActive_background>linear-gradient(to bottom, #005076 0%, #005584 100%)</navigationActive_background>
                <navigationActive_backgroundColor>#005584</navigationActive_backgroundColor>
                <navigationActive_backgroundRepeat>repeat-x</navigationActive_backgroundRepeat>
                <headerContent_background>linear-gradient(to bottom, #c8e3ef 0%, #ffffff 100%)</headerContent_background>
                <headerContent_backgroundColor>#FFFFFF</headerContent_backgroundColor>
                <headerContent_backgroundRepeat>repeat-x</headerContent_backgroundRepeat>
                <headerContent_borderTop>1px solid #ffffff</headerContent_borderTop>
                <breadcrumbs_background>#fff none</breadcrumbs_background>
                <breadcrumbs_backgroundColor>#FFFFFF</breadcrumbs_backgroundColor>
                <breadcrumbs_backgroundRepeat>repeat</breadcrumbs_backgroundRepeat>
                <pageMain_background>#FFFFFF</pageMain_background>
                <pageBottom_background>linear-gradient(to bottom, #c8e3ef 0%, #ffffff 20%, #ffffff 100%)</pageBottom_background>
                <pageBottom_backgroundColor>transparent</pageBottom_backgroundColor>
                <pageBottom_backgroundRepeat>repeat</pageBottom_backgroundRepeat>
                <pageBottom_borderTop>1px solid #ffffff</pageBottom_borderTop>
                <primaryButtons_background>#404040 linear-gradient(to bottom, #707070 0%, #404040 100%)</primaryButtons_background>
                <primaryButtons_backgroundColor>#404040</primaryButtons_backgroundColor>
                <primaryButtons_backgroundRepeat>repeat-x</primaryButtons_backgroundRepeat>
                <primaryButtonsHover_background>#404040 linear-gradient(to bottom, #505050 0%, #404040 100%)</primaryButtonsHover_background>
                <primaryButtonsHover_backgroundColor>#404040</primaryButtonsHover_backgroundColor>
                <primaryButtonsHover_backgroundRepeat>repeat-x</primaryButtonsHover_backgroundRepeat>
            </background>
            <navigation>
                <navigationTopLinks_color>#c3d5e3</navigationTopLinks_color>
                <navigationSeparators_borderRight>1px solid #046c96</navigationSeparators_borderRight>
                <navigationSeparators_boxShadow>1px 0 #103261</navigationSeparators_boxShadow>
            </navigation>
            <footer>
                <footerAdditional_borderTop>1px solid #136793</footerAdditional_borderTop>
                <footerAdditional_borderBottom>1px solid #136793</footerAdditional_borderBottom>
                <switcherStore_backgroundColor>#00628e</switcherStore_backgroundColor>
            </footer>
            <fonts>
                <blockTitle_color>#459b06</blockTitle_color>
                <pageTitle_color>#459b06</pageTitle_color>
                <productLink_color>#68a8e0</productLink_color>
                <footerContacts_color>#FFFFFF</footerContacts_color>
                <footerInformationalHeadings_color>#d5e1e9</footerInformationalHeadings_color>
                <footerInformationalLinks_color>#d5e1e9</footerInformationalLinks_color>
            </fonts>
            <css_selector>
                <body>body</body>
                <footer>.page-footer</footer>
                <footer-additional>.page-footer .footer-additional</footer-additional>
                <switcher-store>.page-footer .footer.content .switcher-store</switcher-store>
                <navigation>.navigation</navigation>
                <navigation-ul><![CDATA[.navigation > ul]]></navigation-ul>
                <navigation-top-links><![CDATA[.navigation .level0 > .level-top]]></navigation-top-links>
                <navigation-separators>.navigation .level0</navigation-separators>
                <navigation-active>.navigation .level0.active, .navigation .level0.has-active</navigation-active>
                <header-content>.header.content</header-content>
                <breadcrumbs>.breadcrumbs</breadcrumbs>
                <page-main>.page-main</page-main>
                <page-bottom>.page-bottom</page-bottom>
                <primary-buttons>.action.primary</primary-buttons>
                <primary-buttons-hover>.action.primary:hover</primary-buttons-hover>
                <block-title>.page-main .main .block-title, .page-bottom .block-title strong</block-title>
                <page-title>.page-title .base</page-title>
                <product-link>.product-item .product-item-name a</product-link>
                <footer-contacts>.page-footer .footer-contacts</footer-contacts>
                <footer-informational-headings><![CDATA[.page-footer .informational > ul > li h4]]></footer-informational-headings>
                <footer-informational-links><![CDATA[.page-footer .informational > ul > li ul li a]]></footer-informational-links>
            </css_selector>
        </swissup_absolute>
        <!-- Absolute Theme Default Values END -->

        <!-- Argento Essence Default Values START -->
        <swissup_argento_essence>
            <layout>
                <body_backgroundColor>#fff</body_backgroundColor>
                <body_backgroundImage></body_backgroundImage>
                <body_backgroundRepeat>no-repeat</body_backgroundRepeat>
                <body_backgroundPosition>0,0</body_backgroundPosition>
                <page_maxWidth>none</page_maxWidth>
                <contentContainer_maxWidth>980px</contentContainer_maxWidth>
            </layout>
            <header>
                <topToolbarContainer>
                    <topToolbarContainer_backgroundColor>#f2f2f2</topToolbarContainer_backgroundColor>
                    <topToolbarContainer_backgroundImage>noisy_texture.png</topToolbarContainer_backgroundImage>
                    <topToolbarContainer_backgroundRepeat>repeat</topToolbarContainer_backgroundRepeat>
                    <topToolbarContainer_backgroundPosition>0,0</topToolbarContainer_backgroundPosition>
                    <topToolbarText_color>#7e807e</topToolbarText_color>
                    <topToolbarText_fontSize>14px</topToolbarText_fontSize>
                </topToolbarContainer>
                <navigation>
                    <navigationContainer_backgroundColor>transparent</navigationContainer_backgroundColor>
                    <navigationContainer_backgroundImage>nav_container.gif</navigationContainer_backgroundImage>
                    <navigationContainer_backgroundRepeat>no-repeat</navigationContainer_backgroundRepeat>
                    <navigationContainer_backgroundPosition>50%,100%</navigationContainer_backgroundPosition>
                    <topLevelLi_borderLeftColor>rgba(0, 0, 0, 0.2)</topLevelLi_borderLeftColor>
                    <topLevelLi_borderRightColor>rgba(255, 255, 255, 1)</topLevelLi_borderRightColor>
                    <topLevelLink_fontFamily>'Open Sans', 'Helvetica Neue', Helvetica, Arial, sans-serif</topLevelLink_fontFamily>
                    <topLevelLink_fontSize>1.4rem</topLevelLink_fontSize>
                    <topLevelLink_textTransform>none</topLevelLink_textTransform>
                    <topLevelLink_color>#303030</topLevelLink_color>
                    <topLevelLinkHover_color>#333333</topLevelLinkHover_color>
                    <topLevelLinkActive_color>#333333</topLevelLinkActive_color>
                    <topLevelArrow_fontSize>14px</topLevelArrow_fontSize>
                    <lowLevelLink_fontFamily>'Open Sans', 'Helvetica Neue', Helvetica, Arial, sans-serif</lowLevelLink_fontFamily>
                    <lowLevelLink_fontSize>1.4rem</lowLevelLink_fontSize>
                    <lowLevelLink_textTransform>none</lowLevelLink_textTransform>
                    <lowLevelLink_backgroundColor>#ffffff</lowLevelLink_backgroundColor>
                    <lowLevelLinkHover_backgroundColor>#efefef</lowLevelLinkHover_backgroundColor>
                    <lowLevelLinkActive_backgroundColor>#efefef</lowLevelLinkActive_backgroundColor>
                    <lowLevelLink_color>#303030</lowLevelLink_color>
                    <lowLevelLinkHover_color>#303030</lowLevelLinkHover_color>
                    <lowLevelLinkActive_color>#d96708</lowLevelLinkActive_color>
                    <lowLevelArrow_fontSize>14px</lowLevelArrow_fontSize>
                </navigation>
            </header>
            <footer>
                <topToolbarContainer>
                    <footerTopToolbar_backgroundColor>#f2f2f2</footerTopToolbar_backgroundColor>
                    <footerTopToolbar_backgroundImage>noisy_texture.png</footerTopToolbar_backgroundImage>
                    <footerTopToolbar_backgroundRepeat>no-repeat</footerTopToolbar_backgroundRepeat>
                    <footerTopToolbar_backgroundPosition>0px,0px</footerTopToolbar_backgroundPosition>
                    <footerTopToolbarText_color>#333333</footerTopToolbarText_color>
                </topToolbarContainer>
                <footer>
                    <footer_backgroundColor>transparent</footer_backgroundColor>
                    <footer_backgroundImage>none</footer_backgroundImage>
                    <footer_backgroundRepeat>no-repeat</footer_backgroundRepeat>
                    <footer_backgroundPosition>0px,0px</footer_backgroundPosition>
                    <footerText_color>#333333</footerText_color>
                    <footerLinks_color>#575757</footerLinks_color>
                </footer>
                <bottomToolbarContainer>
                    <footerBottomToolbar_backgroundColor>#7e807e</footerBottomToolbar_backgroundColor>
                    <footerBottomToolbar_backgroundImage>none</footerBottomToolbar_backgroundImage>
                    <footerBottomToolbar_backgroundRepeat>no-repeat</footerBottomToolbar_backgroundRepeat>
                    <footerBottomToolbar_backgroundPosition>0px,0px</footerBottomToolbar_backgroundPosition>
                    <footerBottomToolbarText_color>#ffffff</footerBottomToolbarText_color>
                </bottomToolbarContainer>
            </footer>
            <tabs>
                <tab_padding>1px 35px 1px 35px</tab_padding>
                <tab_fontFamily>'Open Sans', 'Helvetica Neue', Helvetica, Arial, sans-serif</tab_fontFamily>
                <tab_fontSize>1.4rem</tab_fontSize>
                <tab_fontWeight>400</tab_fontWeight>
                <tab_textTransform>none</tab_textTransform>
                <tab_color>#666666</tab_color>
                <tabBorder_borderColor>#cfe2f1</tabBorder_borderColor>
                <tab_background>#ffffff</tab_background>
                <tabHover_background>#ffffff</tabHover_background>
                <tabHover_color>#666666</tabHover_color>
                <tabActive_background>#ffffff</tabActive_background>
                <tabActive_color>#d96708</tabActive_color>
            </tabs>
            <buttons>
                <button_background>linear-gradient(to bottom, #ff963b 0%, #ff7a0e 100%)</button_background>
                <button_color>#ffffff</button_color>
                <buttonHover_background>linear-gradient(to bottom, false 0%, false 100%)</buttonHover_background>
                <buttonHover_color>#ffffff</buttonHover_color>
                <buttonSecondary_background>#a3a3a3</buttonSecondary_background>
                <buttonSecondary_color>#ffffff</buttonSecondary_color>
                <buttonSecondaryHover_background>#9e9e9e</buttonSecondaryHover_background>
                <buttonSecondaryHover_color>#ffffff</buttonSecondaryHover_color>
            </buttons>
            <fonts>
                <pageTitle_fontFamily>'Open Sans', 'Helvetica Neue', Helvetica, Arial, sans-serif</pageTitle_fontFamily>
                <pageTitle_fontSize>40px</pageTitle_fontSize>
                <pageTitle_fontWeight>300</pageTitle_fontWeight>
                <pageTitle_color>#333333</pageTitle_color>
                <blockTitle_fontFamily>'Open Sans', 'Helvetica Neue', Helvetica, Arial, sans-serif</blockTitle_fontFamily>
                <blockTitle_fontSize>18px</blockTitle_fontSize>
                <blockTitle_fontWeight>300</blockTitle_fontWeight>
                <blockTitle_color>#333333</blockTitle_color>

                <productNameLink_fontFamily>'Open Sans', 'Helvetica Neue', Helvetica, Arial, sans-serif</productNameLink_fontFamily>
                <productNameLink_fontSize>1.4rem</productNameLink_fontSize>
                <productNameLink_fontWeight>400</productNameLink_fontWeight>
                <productNameLink_color>#333333</productNameLink_color>
                <productNameLinkHover_color>#333333</productNameLinkHover_color>
            </fonts>
            <css_selector>
                <body>body</body>
                <page>.page-wrapper</page>
                <top-toolbar-container>.page-header .panel.wrapper</top-toolbar-container>
                <top-toolbar-text><![CDATA[.page-header .panel.wrapper, .header.panel > .header.links > li, .header.panel > .header.links > li > a, .header.panel > .header.links > li > a:hover, .header.panel > .header.links > li > a:visited]]></top-toolbar-text>
                <header-container>.page-header .header.content, .page-wrapper .nav-sections .nav-sections-items</header-container>
                <navigation-container>.nav-sections</navigation-container>
                <nav>.page-wrapper .nav-sections .navigation</nav>
                <content-container><![CDATA[.navigation, .breadcrumbs, .page-header .header.panel, .header.content, .footer.content, .page-wrapper > .widget, .page-wrapper > .page-bottom, .block.category.event, .top-container, .page-main, .page-container > .content]]></content-container>
                <footer>.page-footer</footer>
                <footer-text>.footer.content</footer-text>
                <footer-links>.footer.content a, .footer.content a:hover, .footer.content a:visited, .footer.content .links a, .footer.content .links a:hover, .footer.content .links a:visited</footer-links>
                <footer-top-toolbar>.page-footer .footer-toolbar-top</footer-top-toolbar>
                <footer-top-toolbar-text>.footer-toolbar-top, .footer-toolbar-top a</footer-top-toolbar-text>
                <footer-content>.page-footer .footer.content</footer-content>
                <footer-bottom-toolbar>.page-footer .footer-toolbar-bottom</footer-bottom-toolbar>
                <footer-bottom-toolbar-text>.page-footer .footer-toolbar-bottom, .footer-toolbar-bottom a, .footer-toolbar-bottom .alink</footer-bottom-toolbar-text>
                <top-level-link>.nav-sections .navigation a.level-top</top-level-link>
                <top-level-link-hover><![CDATA[.navigation .level0 > .level-top:hover,.navigation .level0 > .level-top.ui-state-focus]]></top-level-link-hover>
                <top-level-link-active><![CDATA[.navigation .level0.active > .level-top,.navigation .level0.has-active > .level-top]]></top-level-link-active>
                <top-level-arrow>.nav-sections .navigation .parent a.level-top:after</top-level-arrow>
                <top-level-li>.navigation .level0</top-level-li>
                <low-level-link>.nav-sections .navigation .level0 .submenu a</low-level-link>
                <low-level-link-hover>.nav-sections .navigation .level0 .submenu a:hover,.navigation .level0 .submenu a.ui-state-focus</low-level-link-hover>
                <low-level-link-active><![CDATA[.nav-sections .navigation .level0 .submenu .active > a]]></low-level-link-active>
                <low-level-arrow><![CDATA[.nav-sections .navigation .parent > a:after]]></low-level-arrow>
                <tab-border><![CDATA[.product.data.items > .item.content, .product.data.items > .item.title > .switch, .argento-tabs > .item.content, .argento-tabs > .item.title > .switch]]></tab-border>
                <tab><![CDATA[.product.data.items > .item.title > .switch, .argento-tabs > .item.title > .switch]]></tab>
                <tab-hover><![CDATA[.product.data.items > .item.title:not(.disabled) > .switch:focus,.product.data.items > .item.title:not(.disabled) > .switch:hover,.argento-tabs > .item.title:not(.disabled) > .switch:focus,.argento-tabs > .item.title:not(.disabled) > .switch:hover]]></tab-hover>
                <tab-active><![CDATA[.product.data.items > .item.title:not(.disabled) > .switch:active,.product.data.items > .item.title.active > .switch,.product.data.items > .item.title.active > .switch:focus,.product.data.items > .item.title.active > .switch:hover, .argento-tabs > .item.title:not(.disabled) > .switch:active,.argento-tabs > .item.title.active > .switch,.argento-tabs > .item.title.active > .switch:focus,.argento-tabs > .item.title.active > .switch:hover]]></tab-active>
                <button>.action.primary</button>
                <button-hover>.action.primary:hover, .action.primary:focus, .action.primary:active</button-hover>
                <button-secondary><![CDATA[button, .abs-revert-to-action-secondary, .paypal-review .block .actions-toolbar .action.primary, .abs-discount-block .actions-toolbar .action.primary, .abs-discount-block .actions-toolbar .action.cancel, .block-compare .action.primary, .cart-summary .block .actions-toolbar > .primary .action.primary, .cart-summary .form.giftregistry .fieldset .action.primary, .paypal-review-discount .actions-toolbar .action.primary, .paypal-review-discount .actions-toolbar .action.cancel, .cart-discount .actions-toolbar .action.primary, .cart-discount .actions-toolbar .action.cancel, .review-form .action.submit.primary]]></button-secondary>
                <button-secondary-hover><![CDATA[button:hover, .abs-revert-secondary-color:hover, .abs-revert-to-action-secondary:hover, .paypal-review .block .actions-toolbar .action.primary:hover,.abs-discount-block .actions-toolbar .action.primary:hover, .abs-discount-block .actions-toolbar .action.cancel:hover, .block-compare .action.primary:hover, .cart-summary .block .actions-toolbar > .primary .action.primary:hover, .cart-summary .form.giftregistry .fieldset .action.primary:hover, .paypal-review-discount .actions-toolbar .action.primary:hover, .paypal-review-discount .actions-toolbar .action.cancel:hover, .cart-discount .actions-toolbar .action.primary:hover, .cart-discount .actions-toolbar .action.cancel:hover, .review-form .action.submit.primary:hover]]></button-secondary-hover>
                <page-title>h1, h1.page-title</page-title>
                <block-title>.sidebar .block .block-title span strong, .sidebar .block .block-title span span, .sidebar .block .block-title strong, .sidebar .block .block-title span, .block .block-title strong</block-title>
                <product-name-link>.products-grid .product-item-name a, .products-list .product-item-name a</product-name-link>
                <product-name-link-hover>.products-grid .product-item-name a:hover, .products-list .product-item-name a:hover</product-name-link-hover>
            </css_selector>
        </swissup_argento_essence>
        <!-- Argento Essence Default Values END -->

    </default>
</config>
